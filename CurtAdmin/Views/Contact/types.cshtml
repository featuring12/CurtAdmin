@{
    ViewBag.Title = "Contacts";
    ViewBag.navigation = "_WebsiteNavigation";
    Layout = "~/Views/Shared/_Layout.cshtml";
    List<CurtAdmin.ContactType> types = ViewBag.types;
}
<script type="text/javascript" src="@Url.Content("~/Scripts/jquery.dataTables.min.js")"></script>
<ul id="subNav">
    <li>
        <a href="@Url.Content("~/Contact")">Contacts</a>
    </li>
    <li>
        <a href="@Url.Content("~/Contact/Receivers")">Receivers</a>
    </li>
    <li>
        <a href="@Url.Content("~/Contact/Types")" class="active">Types</a>
    </li>
    <li class="add">
        <a href="@Url.Content("~/Contact/AddType")">Add Type</a>
    </li>
    <li class="add">
        <a href="@Url.Content("~/Contact/AddReceiver")">Add Receiver</a>
    </li>
    <li class="add">
        <a href="@Url.Content("~/Contact/Export")">Export</a>
    </li>
</ul>
<div id="content_container">
    <h2>Manage Contact Types</h2>
    <table style="width: 100% !important">
        <thead>
            <tr>
                <th>Name</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (CurtAdmin.ContactType t in types) {
                <tr id="type:@t.contactTypeID">
                    <td>@t.name</td>
                    <td class="center">
                        <a href="/Contact/DeleteType/@t.contactTypeID">Delete</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        var contacttable = $('table').dataTable({
            "bJQueryUI": true
        });
    });
</script>