@{
    ViewBag.Title = "Index";
    ViewBag.navigation = "_VehicleNavigation";
    Layout = "~/Views/Shared/_Layout.cshtml";
    List<CurtAdmin.vcdb_Make> makes = ViewBag.makes ?? new List<CurtAdmin.vcdb_Make>();
}
<ul id="subNav">
    <li>
        <a href="@Url.Content("~/ACES/Vehicles")" class="active">View Vehicles</a>
    </li>
    <li>
        <a href="@Url.Content("~/ACES/ConfigAttributes")">View Configuration Attributes</a>
    </li>
    <li>
        <a href="@Url.Content("~/ACES/ConfigTypes")">View Configuration Types</a>
    </li>
    <li>
        <a href="@Url.Content("~/ACES/ACESTypes")">View ACES Types</a>
    </li>
    <li class="add">
        <a href="@Url.Content("~/ACES/AddVehicle")">Add New</a>
    </li>
</ul>
<div id="content_container">
    <h2>Manage ACES Vehicle Data</h2>
    <select id="make" name="make">
        <option value="">Select a Make</option>
        @foreach (CurtAdmin.vcdb_Make make in makes) {
            <option value="@make.ID">@make.MakeName</option>
        }
    </select>

    <select id="model" name="model" disabled="disabled">
        <option value="">Select a Model</option>
    </select>
    <button id="find">Find</button>
    <div class="clear"></div>
    <div id="tabs">
        <ul class="tabs"><li><a href="#CurtDev">CurtDev</a></li><li><a href="#vcdb">VCDB</a></li></ul>
        <div id="CurtDev">
            <img id="loadingCurtDev" src="/Content/img/ajax_loader.gif" alt="Loading..." style="display: none;" />
            <ul class="vehicles" id="vehicleData"></ul>
        </div>
        <div id="vcdb">
            <img id="loadingVCDB" src="/Content/img/ajax_loader.gif" alt="Loading..." style="display: none;" />
            <ul class="vehicles" id="vcdbData"></ul>
        </div>
    </div>
</div>
<script type="text/javascript" src="/Scripts/admin/aces.js"></script>